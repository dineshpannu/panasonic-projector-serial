<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="ServerPort" value="8889"/>
    <add key="ComPort" value="COM1"/>

    <!-- 
      Aspect ratios each lens memory is intended to be used by.
      Can be entered as:
        1.78:1
        16x9
        1.78
    -->
    <add key="LensMemory1" value="1.78:1"/>
    <add key="LensMemory2" value="1.85:1"/>
    <add key="LensMemory3" value="2.35:1"/>
    <add key="LensMemory4" value="2.00:1"/>
    <add key="LensMemory5" value="2.20:1"/>
    <add key="LensMemory6" value="0"/>
  </appSettings>
</configuration>